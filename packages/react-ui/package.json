{
  "name": "@whereby.com/react-ui",
  "version": "0.0.0",
  "description": "React UI components for Whereby",
  "author": "Whereby AS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/whereby/sdk.git",
    "directory": "packages/react-ui"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@radix-ui/react-avatar": "^1.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.4.0",
    "@storybook/addon-essentials": "^8.1.4",
    "@storybook/addon-interactions": "^8.1.4",
    "@storybook/addon-links": "^8.1.4",
    "@storybook/addon-onboarding": "^8.1.4",
    "@storybook/blocks": "^8.1.4",
    "@storybook/react": "^8.1.4",
    "@storybook/react-vite": "^8.1.4",
    "@storybook/test": "^8.1.4",
    "eslint-plugin-storybook": "^0.8.0",
    "storybook": "^8.1.4"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "dev": "storybook dev -p 6007",
    "build": "rollup -c rollup.config.js",
    "test": "yarn test:lint && yarn test:unit",
    "test:lint": "eslint src/",
    "test:unit": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js",
    "test:unit:watch": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --watch",
    "test:attw": "attw --pack . -f table --ignore-rules fallback-condition unexpected-module-syntax",
    "storybook:dev": "storybook dev -p 6007 --ci",
    "build-storybook": "storybook build"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.cjs",
  "module": "dist/legacy-esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.mjs",
    "dist/**/*.cjs",
    "dist/**/*.d.ts",
    "dist/**/*.d.mts",
    "dist/**/*.d.cts"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
