{
  "name": "@whereby.com/react-native-sdk",
  "version": "0.1.0",
  "description": "Modules for integration Whereby video in react native apps",
  "source": "./src/index.tsx",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    }
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "test": "yarn test:typescript && yarn test:lint && yarn test:unit",
    "test:typescript": "tsc",
    "test:lint": "eslint src/",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:attw": "attw --pack . -f table --ignore-rules fallback-condition unexpected-module-syntax",
    "clean": "del-cli lib",
    "build": "bob build"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whereby/sdk.git.git"
  },
  "author": "Whereby AS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/whereby/sdk.git/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@react-native/eslint-config": "^0.73.1",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.44",
    "del-cli": "^5.1.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-builder-bob": "^0.30.0",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "create-react-native-library": {
    "type": "library",
    "version": "0.41.0"
  }
}
